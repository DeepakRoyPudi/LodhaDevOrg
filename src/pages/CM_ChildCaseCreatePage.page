	<script>
	    
	    //Function to clear L3
	    function clearl3(sectionNo){
	        var l3B1 = document.getElementById('pageid:mainform:blkActionItem:Request_Details:secL3B1:secItemL3B1:RequestForL3b_selected');
	        var l3A1 = document.getElementById('pageid:mainform:blkActionItem:Request_Details:secL3A1:secItemL3A1:RequestForL3a_selected');
	        var l3B2 = document.getElementById('pageid:mainform:blkChildCase:Request_Details2:secL3B2:secItemL3B2:RequestForL3b2_selected');
	        var l3A2 = document.getElementById('pageid:mainform:blkChildCase:Request_Details2:secL3A2:secItemL3A2:RequestForL3a2_selected');
	        
	        if(l3B1 !== null)
	        {	
	        	var l3Blenght = l3B1.length;
		      	for (var i = 0; i < l3Blenght; i++) 
		          	l3B1.options.remove(0);
		     }
		     	
	     	if(l3A1 !== null)
	     	{
	     		var l3Alenght = l3A1.length;
		      	for (var i = 0; i < l3Alenght; i++) 
					l3A1.options.remove(0);
		    }
		    
		    if(l3B2 !== null)
	        {	
	        	var l3B2length = l3B2.length;
		      	for (var i = 0; i < l3B2length; i++) 
		          	l3B2.options.remove(0);
		     }
		     	
	     	if(l3A2 !== null)
	     	{
	     		var l3A2length = l3A2.length;
		      	for (var i = 0; i < l3A2length; i++) 
					l3A2.options.remove(0);
		    }
		    
		    if(sectionNo == 1)
				callonchange();
			else if(sectionNo == 2)
				callonchange2();
	    }
	    
	    //Added by Anupam: 
	    var L2Id;
	    function sendCombinationB()
	    {
	    	var select = document.getElementById('pageid:mainform:blkActionItem:Request_Details:secL3B1:secItemL3B1:RequestForL3b_selected');
	      	var selected = [ ];
	      	for (var i = 0; i < select.length; i++) 
	          selected.push(select.options[i].label);
	     	
	     	var l3BValues = "\'" + selected.join("\',\'") + "\'";
	    	callOnchangeB(document.getElementById(L2Id).value, l3BValues);
	    }
	    
	    function sendCombinationA()
	    {
	    	var select = document.getElementById('pageid:mainform:blkActionItem:Request_Details:secL3A1:secItemL3A1:RequestForL3a_selected');
	      	var selected = [ ];
	      	for (var i = 0; i < select.length; i++)
	          selected.push(select.options[i].label);
	     	
	     	var l3AValues = "\'" + selected.join("\',\'") + "\'";
	    	callOnchangeA(document.getElementById(L2Id).value, l3AValues);
	    }
	    
	    var L2Id2;
	    function sendCombinationB2()
	    {
	    	var select = document.getElementById('pageid:mainform:blkChildCase:Request_Details2:secL3B2:secItemL3B2:RequestForL3b2_selected');
